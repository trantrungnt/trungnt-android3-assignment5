<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="trungnt.gen3.techkids.circleprogressbar.MainActivity">

    <EditText
        android:id="@+id/editTextTaskName"
        android:hint="@string/editTextTaskName_hint"
        android:gravity="center"
        android:layout_marginTop="@dimen/editTextTaskName_marginTop"
        android:layout_centerHorizontal="true"
        android:layout_width="@dimen/editTextTaskName_width"
        android:layout_height="@dimen/editTextTaskName_height" />

    <trungnt.gen3.techkids.circleprogressbar.CircleProgressBar
        android:id="@+id/circleProgressBar"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/editTextTaskName"
        android:layout_width="@dimen/circleProgressBar_width"
        android:layout_height="@dimen/circleProgressBar_height" />

    <ImageButton
        android:id="@+id/imgBtnProcess"
        android:layout_below="@id/circleProgressBar"
        android:layout_centerHorizontal="true"
        android:layout_width="@dimen/imgBtnProcess_width"
        android:layout_height="@dimen/imgBtnProcess_height" />
</RelativeLayout>
